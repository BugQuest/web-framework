{
  "name": "bugquest/web-framework",
  "description": "BugQuest Web Framework",
  "keywords": [
    "bugquest",
    "framework"
  ],
  "authors": [
    {
      "name": "Realitynauts"
    }
  ],
  "type": "project",
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "composer/installers": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.4",
    "larapack/dd": "^1.1",
    "twig/twig": "^3.20",
    "vlucas/phpdotenv": "^5.6",
    "illuminate/database": "^12.8",
    "ext-iconv": "*",
    "ext-imagick": "*",
    "ext-curl": "*"
  },
  "autoload": {
    "psr-4": {
      "BugQuest\\Framework\\": "/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^12.1"
  }
}
